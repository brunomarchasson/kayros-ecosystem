{
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  // "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": false,
  "editor.tabSize": 2,
  "eslint.nodePath": ".yarn/sdks",
  "prettier.prettierPath": ".yarn/sdks/prettier/index.js",
  "jest.jestCommandLine": "yarn jest",
  "jestrunner.enableYarnPnpSupport": true,
  "files.associations": {
    ".terraformrc": "terraform"
  },
  "files.exclude": {
    ".cache": true,
    ".editorconfig": true,
    ".eslintcache": true,
    ".git": true,
    ".gitattributes": true,
    ".husky": true,
    ".prettierignore": true,
    ".yarn": true,
    "LICENSE": true,
    "yarn.lock": true
  },
  "search.exclude": {
    "**/dist/": true,
    "**/yarn-error.log": true,
    "**/yarn.lock": true,
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "cSpell.ignoreWords": [
    "abcdefghijklmnopqrstuvwxyz",
    "browserslist",
    "citext",
    "cloudfunctions",
    "cloudsql",
    "corejs",
    "dataloader",
    "datname",
    "devtool",
    "downlevel",
    "entrypoint",
    "envalid",
    "envars",
    "eslintcache",
    "esmodules",
    "esnext",
    "execa",
    "firestore",
    "gamecenter",
    "gcloud",
    "globby",
    "GraphQLID",
    "gsutil",
    "gtag",
    "jsonb",
    "knexfile",
    "kriasoft",
    "linkedin",
    "miniflare",
    "nothrow",
    "octokit",
    "pathinfo",
    "pgappname",
    "pgdatabase",
    "pgdebug",
    "pghost",
    "pgpassword",
    "pgport",
    "pgservername",
    "pgsslcert",
    "pgsslkey",
    "pgsslmode",
    "pgsslrootcert",
    "pguser",
    "playgames",
    "pnpify",
    "postgres",
    "psql",
    "refetchable",
    "sendgrid",
    "sourcemap",
    "spdx",
    "tfvars",
    "timestamptz",
    "tslib",
    "typechecking",
    "unprocessable",
    "upsert",
    "webflow",
    "yarnpkg",
    "yarnrc"
  ],
  "cSpell.words": [
    "kayros"
  ]
}
